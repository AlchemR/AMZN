<!DOCTYPE html>
<html>
  <head>
    <title>AMZN</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
        <script type='text/javascript'>
        window.amzn_logo = "<%=image_url('amzn_logo_6.png') %>"
        window.ca_flag = "<%=image_url('ca_flag.jpg') %>"
        window.usa_flag = "<%=image_url('usa_flag.jpg') %>"
        window.linkedin_logo = "<%=image_url('linkedin-logo.png') %>"
        window.github_logo = "<%=image_url('github-logo.png') %>"
        window.angels_list = "<%=image_url('angels_list.png') %>"
        window.portfolio = "<%=image_url('portfolio.png') %>"

        window.world_meridian = "<%=image_url('world-meridian.png') %>"
        window.cart_image = "<%=image_url('shopping_cart.png') %>"
        window.default_profile = "<%=image_url('default_profile.png') %>"
        window.prime_shipping = "<%=image_url('amazon_prime.png') %>"

        </script>

    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>

  </body>
</html>
